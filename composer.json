{
    "name": "ornicar/gravatar-bundle",
    "description": "This bundles provides a Gravatar API various utilities to work with it in templates",
    "keywords": ["symfony", "gravatar", "templating", "twig"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name" : "Thibault Duplessis",
        "email" : "thibault.duplessis@gmail.com"
    },
    {
        "name" : "Henrik BjÃ¸rnskov",
        "email" : "henrik@bjrnskov.dk"
    }],
    "require": {
        "php": ">=5.3.0",
        "symfony/expression-language": "^2.4|^3.0|^4.0",
        "symfony/framework-bundle": "~4.0|~3.0|~2.4",
        "symfony/templating": "~4.0|~3.0|~2.0"
    },
    "require-dev": {
        "doctrine/doctrine-cache-bundle": "^1.3",
        "symfony/phpunit-bridge": "~2.7",
        "twig/extensions": "1.0.*"
    },
    "suggest" : {
        "twig/twig": "For gravatar twig extension usage",
        "doctrine/doctrine-cache-bundle": "To cache gravatar data"
    },
    "conflict": {
        "twig/twig": "<1.12"
    },
    "autoload": {
        "psr-0": {
            "Ornicar\\GravatarBundle\\": ""
        }
    },
    "target-dir" : "Ornicar/GravatarBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
